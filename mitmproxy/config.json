{
  "name": "mitmproxy",
  "version": "1.0.1",
  "slug": "mitmproxy",
  "panel_icon": "mdi:lan-connect",
  "description": "A free and open source interactive HTTPS proxy for intercepting and inspecting network traffic",
  "url": "https://github.com/Poeschl/Hassio-Addons/tree/master/mitmproxy",
  "ingress": false,
  "ingress_port": 8081,
  "arch": [
    "armv7",
    "armhf",
    "aarch64",
    "amd64",
    "i386"
  ],
  "startup": "system",
  "boot": "auto",
  "hassio_api": true,
  "image": "poeschl/ha-mitmproxy-{arch}",
  "map": ["ssl", "share:rw"],
  "ports": {
    "8081/tcp": 8081,
    "8080/tcp": 8080
  },
  "ports_description": {
    "8081/tcp": "Web Frontend",
    "8080/tcp": "Proxy listening port"
  },
  "options": {
    "options": [{ "name": "anticache", "value": "true" }]
  },
  "schema": {
    "options": [{ "name": "str", "value": "str" }],    
    "custom_ca": "match(^/ssl/.*)?"
  }
}
